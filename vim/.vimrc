set nocompatible
set nomodeline
set paste
set hlsearch
set ic
set backspace=2
set bs=indent,eol,start
syntax on
"colorscheme desert
set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
set number

syntax enable
set background=dark
"colorscheme solarized

set cursorline
:hi CursorLine ctermbg=Black cterm=bold guifg=white guibg=yellow gui=bold


:nmap <F12> :set invnumber<CR>
